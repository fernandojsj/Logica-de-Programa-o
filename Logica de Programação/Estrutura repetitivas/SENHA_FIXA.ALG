Algoritmo "senha_fixa"
//  
//  

Var

    senha : inteiro

Inicio
      
      Escreva("Digite a senha: ")
      leia(senha)

      Enquanto senha <> 2005 faca
           Escreval("Senha Invalida! Tente novamente")
           Escreval()
           Escreva("Digite a senha novamente:")
           leia(senha)
      Fimenquanto
      
      Escreva("Acesso permitido!")
      
Fimalgoritmo